{
    "requestTimeoutEnabled": true,
    "whitespaceManagement": "white-space-pref",
    "requestTimeout": "0,0,5,0",
    "cacheDefaultObject": "coldbox",
    "caches": {
        "coldbox": {
            "storage": "true",
            "type": "RAM",
            "custom": {
                "timeToIdleSeconds": "1800",
                "timeToLiveSeconds": "3600"
            },
            "class": "lucee.runtime.cache.ram.RamCache",
            "readOnly": "false"
        }
    },
    "datasources": {
        "${DB_NAME}": {
            "allowAlter": true,
            "allowCreate": true,
            "allowDelete": true,
            "allowDrop": true,
            "allowGrant": true,
            "allowInsert": true,
            "allowRevoke": true,
            "allowSelect": true,
            "allowStoredProcs": true,
            "allowUpdate": true,
            "blob": false,
            "blobBuffer": 64000,
            "class": "${DB_CLASS}",
            "clientApplicationName": false,
            "clientApplicationNamePrefix": "",
            "clientHostname": false,
            "clientUsername": false,
            "clob": false,
            "clobBuffer": 64000,
            "connectionLimit": -1,
            "connectionTimeout": 20,
            "connectionTimeoutInterval": 420,
            "custom": "",
            "database": "${DB_DATABASE}",
            "dbdriver": "${DB_DRIVER}",
            "description": "",
            "disableAutogeneratedKeyRetrieval": false,
            "disableConnections": false,
            "dsn": "${DB_CONNECTIONSTRING}",
            "host": "${DB_HOST}",
            "linkedServers": true,
            "logActivity": false,
            "logActivityFile": "",
            "loginTimeout": 30,
            "maintainConnections": true,
            "maxPooledStatements": 100,
            "password": "${DB_PASSWORD}",
            "port": "${DB_PORT}",
            "queryTimeout": 0,
            "sendStringParametersAsUnicode": false,
            "serviceName": "",
            "SID": "",
            "username": "${DB_USERNAME}",
            "validate": false,
            "validationQuery": ""
        }
    }
}